{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "description": "Jeff Weber's personal portfolio website.",
  "scripts": {
    "dev": "doppler run -- next dev --turbopack",
    "build": "doppler run -- next build --turbopack",
    "vercel-build": "next build",
    "start": "doppler run -- next start",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --max-warnings=0 --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "typecheck": "tsc --noEmit",
    "check": "turbo run lint format:check typecheck",
    "prepare": "husky install",
    "test": "vitest"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.5.1",
    "@supabase/supabase-js": "^2.49.1",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "contentlayer": "^0.3.4",
    "date-fns": "^4.1.0",
    "lucide-react": "^0.544.0",
    "next": "15.5.3",
    "next-contentlayer": "0.3.4",
    "next-mdx-remote": "^5.0.0",
    "next-themes": "^0.4.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.53.0",
    "rehype-autolink-headings": "^7.1.0",
    "rehype-pretty-code": "^0.14.1",
    "rehype-slug": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "remark-gfm": "^4.0.1",
    "remark-parse": "^11.0.0",
    "remark-rehype": "^11.1.2",
    "resend": "^6.1.0",
    "shiki": "^3.12.2",
    "simple-icons": "^15.16.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@hookform/resolvers": "^3.9.0",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "eslint-config-prettier": "^9",
    "husky": "^9",
    "lint-staged": "^15",
    "prettier": "^3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "tailwindcss": "^4",
    "turbo": "^2.5.8",
    "tw-animate-css": "^1.3.8",
    "typescript": "^5",
    "unified": "^11.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "pnpm exec eslint --max-warnings=0 --fix"
    ],
    "*.{js,jsx,ts,tsx,css,md,mdx,json}": [
      "pnpm exec prettier --write"
    ]
  },
  "pnpm": {
    "overrides": {
      "estree-util-value-to-estree": "^3.3.3"
    }
  },
  "engines": {
    "node": "20.x"
  },
  "packageManager": "pnpm@10.17.0"
}
